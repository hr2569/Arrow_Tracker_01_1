{
  "expo": {
    "name": "Arrow Tracker",
    "slug": "arrow-tracker",
    "owner": "hraimundo1",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/7789ca40-95f6-47d2-99d0-6dec6de9edfe"
    },
    "extra": {
      "eas": {
        "projectId": "7789ca40-95f6-47d2-99d0-6dec6de9edfe"
      }
    },
    "version": "2.0.9",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "arrowtracker",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "platforms": [
      "android",
      "web"
    ],
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "android": {
      "package": "com.arrowtracker.app",
      "versionCode": 19,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-image.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "@react-native-community/datetimepicker",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
